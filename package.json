{
  "name": "@khatiwadaprashant/react-animation-kit",
  "version": "1.0.0",
  "description": "ðŸŽ¨ Production-ready React animation library with 142 components built on Framer Motion. Lightweight, tree-shakeable, and perfect for modern web apps.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./animations": {
      "types": "./dist/animations/index.d.ts",
      "import": "./dist/animations/index.js",
      "require": "./dist/animations/index.cjs"
    },
    "./interactive": {
      "types": "./dist/interactive/index.d.ts",
      "import": "./dist/interactive/index.js",
      "require": "./dist/interactive/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "import": "./dist/presets/index.js",
      "require": "./dist/presets/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "size": "size-limit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "animation",
    "framer-motion",
    "motion",
    "react-components",
    "ui",
    "transitions",
    "scroll-animations",
    "page-transitions",
    "interactive",
    "gestures",
    "hooks",
    "typescript",
    "nextjs",
    "ssr",
    "tree-shakeable",
    "lightweight",
    "entrance",
    "exit",
    "hover",
    "tap",
    "loading",
    "accessibility",
    "react-animation",
    "web-animations",
    "ui-animations",
    "animation-library",
    "component-library",
    "react-hooks",
    "modern-ui",
    "user-interface",
    "frontend",
    "web-development"
  ],
  "author": "Prashant Khatiwada <prashantkhatiwada554@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Prashant8Khatiwada/animation-library.git"
  },
  "homepage": "https://github.com/Prashant8Khatiwada/animation-library#readme",
  "bugs": {
    "url": "https://github.com/Prashant8Khatiwada/animation-library/issues"
  },
  "peerDependencies": {
    "framer-motion": ">=10.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/ui": "^1.0.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^10.16.16",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "size-limit": [
    {
      "name": "Single animation component",
      "path": "dist/animations/index.js",
      "limit": "3 KB"
    },
    {
      "name": "Full library",
      "path": "dist/index.js",
      "limit": "20 KB"
    }
  ]
}
